{
  "name": "ecommerce-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "predeploy": "npm run build",
    "deploy": "npm run build && npx gh-pages -d dist -e eCommerce-App",
    "build": "vite build",
    "preview": "vite preview",
    "prettier": "prettier ./src --check",
    "prettier:fix": "prettier ./src --write",
    "lint": "eslint src/ --ext ts",
    "lint:fix": "eslint src/ --fix",
    "formatAll": "npm run lint:fix && npm run prettier:fix",
    "prepare": "husky",
    "tsc": "tsc --noEmit",
    "pre-commit": "npm run prettier",
    "pre-push": "npm run lint && npm run prettier && npm run test && npm run tsc",
    "test:watch": "vitest watch",
    "test": "vitest run"
  },
  "devDependencies": {
    "@commercetools/history-sdk": "^4.3.0",
    "@commercetools/importapi-sdk": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gh-pages": "^6.1.1",
    "happy-dom": "^14.7.1",
    "husky": "^9.0.11",
    "prettier": "3.2.5",
    "sass": "^1.71.1",
    "stylelint": "^16.2.1",
    "stylelint-config-clean-order": "^5.4.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.2.0",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1",
    "validate-branch-name": "^1.3.0",
    "vite": "^5.1.6",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push"
    }
  },
  "dependencies": {
    "@commercetools/platform-sdk": "^7.8.0",
    "@commercetools/sdk-client-v2": "^2.5.0",
    "@control.ts/min": "^0.1.7",
    "@types/uuid": "^9.0.8"
  }
}
